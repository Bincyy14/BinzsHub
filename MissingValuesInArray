package week1.day2;

import java.util.Arrays;

public class MissingValueinArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i;
		int[] arr = {1,3,2,5,7,6};
		Arrays.sort(arr);
		System.out.println(arr.length);
		
		for (i=0;i<arr.length;i++) {
			if(arr[i]!= i+1) {
				System.out.println(i+1);
				break;
			}
		}

	}

}
